{
	"name": "@ldn-viz/web",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "velite && vite build",
		"build:content": "velite",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev && velite dev --watch",
		"dev:content": "velite dev --watch",
		"dev:svelte": "vite dev",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"prepare": "svelte-kit sync || echo ''",
		"preview": "vite preview",
		"test": "echo no tests",
		"test:coverage": "vitest run --coverage",
		"test:e2e": "playwright test",
		"test:unit": "vitest run"
	},
	"dependencies": {
		"@ldn-viz/charts": "*",
		"@ldn-viz/maps": "*",
		"@ldn-viz/tables": "*",
		"@ldn-viz/themes": "*",
		"@ldn-viz/ui": "*",
		"@ldn-viz/utils": "*",
		"mdsvex": "^0.12.6"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@ldn-viz/config-eslint": "*",
		"@ldn-viz/config-typescript": "*",
		"@ldn-viz/config-vitest": "*",
		"@playwright/test": "^1.49.1",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.49.3",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.2",
		"@tailwindcss/vite": "^4.0.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"publint": "^0.3.2",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-pretty-code": "^0.14.0",
		"rehype-slug": "^6.0.0",
		"remark-gfm": "^4.0.0",
		"remove-markdown": "^0.5.5",
		"shiki": "^1.24.0",
		"svelte": "^5.33.0",
		"svelte-check": "^4.1.7",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"unist-util-visit": "^5.0.0",
		"velite": "^0.2.4",
		"vite": "^7.2.2",
		"vitest": "^4.0.16",
		"vitest-browser-svelte": "^2.0.1"
	}
}
