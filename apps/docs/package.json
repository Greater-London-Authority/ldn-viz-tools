		"@storybook/addon-svelte-csf": "^5.0.3",
		"@storybook/addon-themes": "^9.0.1",
    	"@storybook/addon-docs": "^9.0.1",
		"@storybook/addon-vitest": "^9.0.1",
		"@storybook/svelte": "^9.0.1",
		"@storybook/sveltekit": "^9.0.1",
